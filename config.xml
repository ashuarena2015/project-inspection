<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.projectinspection" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Project Inspection</name>
    <description>
      A application based on ReactJs.
  </description>
    <author email="ashuarena@gmail.com" href="http://ideaweaver.in">
      Ideaweaver Team
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="23" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="icon.png" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.2" />
</widget>
